<Window x:Name="startwin" x:Class="HealthApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="197" Width="405" Loaded="startwin_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="56*"/>
            <ColumnDefinition Width="461*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="loginbox" Grid.Column="1" HorizontalAlignment="Left" Height="23" Margin="95,34,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBlock Grid.Column="1" HorizontalAlignment="Left" Margin="126,13,0,0" TextWrapping="Wrap" Text="Логин" VerticalAlignment="Top" Width="66"/>
        <TextBlock Grid.Column="1" HorizontalAlignment="Left" Margin="126,77,0,0" TextWrapping="Wrap" Text="Пароль" VerticalAlignment="Top"/>
        <PasswordBox x:Name="passwordbox" Grid.Column="1" HorizontalAlignment="Left" Margin="95,98,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="enter" Content="Войти" Grid.Column="1" HorizontalAlignment="Left" Margin="117,133,0,0" VerticalAlignment="Top" Width="75" Click="enter_Click"/>
        <Button x:Name="signup" Content="Регистрация" HorizontalAlignment="Left" Margin="22,14,0,0" VerticalAlignment="Top" Width="75" Grid.ColumnSpan="2" Click="signup_Click"/>
    </Grid>
</Window>
